RewriteEngine on
# redirect www to non-www
#RewriteCond %{HTTP_HOST} ^www\.xcitehitec\.com$ [NC]
#RewriteRule ^(.*)$ http://xcitehitec.com/$1 [L,R=301]

RewriteCond %{HTTP_HOST} ^xcitehitec\.com$ [NC]
RewriteRule ^(.*)$ http://www.xcitehitec.com/$1 [L,R=301]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php?$1
ErrorDocument 404 http://www.xcitehitec.com/demo/notfound
